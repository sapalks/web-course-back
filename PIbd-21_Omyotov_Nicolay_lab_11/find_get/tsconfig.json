{
   "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "esModuleInterop": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "target": "es2017",
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": "./"
   },
   "exclude": [
      "node_modules",
      "dist"
   ]
}